<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="100%" height="100%" ui:field="dockLayoutPanel">
		<g:north size="40">
			<g:HorizontalPanel width="100%" >
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" spacing="4">
						<g:MenuBar>
					<g:MenuItem text="Mappings">
						<g:MenuBar vertical="true">
							<g:MenuItem text="Add Mapping" ui:field="miMappingsAdd" />
							<g:MenuItem text="View Mappings" ui:field="miViewMappings" />
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text="Application">
						<g:MenuBar vertical="true">
							<g:MenuItem text="Add" ui:field="miPackageAdd"/>
							<g:MenuItem text="Edit" ui:field="miPackageEdit"/>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text="User">
						<g:MenuBar vertical="true">
							<g:MenuItem text="Edit" ui:field="miUserEdit"/>
						
						</g:MenuBar>
					</g:MenuItem>
				</g:MenuBar>
						<g:Label text="Application" horizontalAlignment="ALIGN_RIGHT"
						wordWrap="false" />
						<g:ListBox ui:field="appsCombo" />
						<g:VerticalPanel verticalAlignment="ALIGN_MIDDLE">
						<g:Label text="New Label" wordWrap="false" horizontalAlignment="ALIGN_LEFT" ui:field="textUserStats"/>
						<g:Label text="New Label" wordWrap="false" horizontalAlignment="ALIGN_LEFT" ui:field="textAppStats"/>
					</g:VerticalPanel>
					</g:HorizontalPanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel spacing="4" verticalAlignment="ALIGN_MIDDLE">
						<g:Cell verticalAlignment="ALIGN_MIDDLE">
							<g:Button ui:field="buttonLogout">Logout</g:Button>
						</g:Cell>
					</g:HorizontalPanel>
				</g:Cell>

			</g:HorizontalPanel>
		</g:north>
<!-- 		<g:west size="280"> -->
<!-- 		</g:west> -->
<!-- 		<g:center> -->
<!-- 		</g:center> -->
	</g:DockLayoutPanel>
</ui:UiBinder> 