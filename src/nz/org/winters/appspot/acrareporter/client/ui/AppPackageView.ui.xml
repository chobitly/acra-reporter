<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="100%" height="100%" ui:field="dockLayoutPanel">
		<g:north size="38.0">
			<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" width="100%" spacing="4">
				<g:Cell verticalAlignment="ALIGN_MIDDLE" width="100%">
					<g:Label text="Crash Reports for: " styleName="gwt-LabelBold" horizontalAlignment="ALIGN_LEFT" ui:field="labelTitle" />
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:Button width="100px" ui:field="buttonEditPackage">Edit App</g:Button>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:Button width="100px" ui:field="buttonMapFiles">Map Files</g:Button>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:Button width="60px" ui:field="buttonClose">Close</g:Button>
				</g:Cell>
			</g:HorizontalPanel>
		</g:north>
	</g:DockLayoutPanel>
</ui:UiBinder> 