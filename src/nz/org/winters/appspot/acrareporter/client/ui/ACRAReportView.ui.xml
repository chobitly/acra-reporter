<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel width="100%" height="100%" styleName="errorlistouter">
		<g:Cell horizontalAlignment="ALIGN_CENTER" height="25">
			<g:Label ui:field="captionPanelCenter" text="report"
				horizontalAlignment="ALIGN_CENTER" styleName="gwt-LabelInvertTop" />
		</g:Cell>
		<g:DockLayoutPanel unit="PX" width="100%" height="100%">
			<g:north size="126">
				<g:FlowPanel width="100%" height="100%">
					<g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>App Version Code:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false"
								ui:field="textAppVersionCode" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>App Version Name:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false"
								ui:field="textAppVersionName" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>

					<g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Phone Model:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textPhoneModel" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Brand:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textBrand" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Product:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textProduct" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Android Version:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false"
								ui:field="textAndroidVersion" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Memory Total/Available:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textMemory" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>App Start Date:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false"
								ui:field="textUserAppStartDate" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Crash Date:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false"
								ui:field="textUserCrashDate" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Device ID:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textDeviceID" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>User E-Mail:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textUserEMail" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>User IP:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textUserIP" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="2">
							<g:Label width="170px" wordWrap="false"
								horizontalAlignment="ALIGN_RIGHT">
								<ui:msg>Silent:</ui:msg>
							</g:Label>
							<g:Label text="" width="170px" wordWrap="false" ui:field="textSilent" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>

					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE"
						spacing="4">
						<g:Cell width="100">
							<g:CheckBox ui:field="checkLookedAt">
								<ui:msg>Looked At</ui:msg>
							</g:CheckBox>
						</g:Cell>
						<g:Cell width="100">
							<g:CheckBox ui:field="checkFixed">
								<ui:msg>Fixed</ui:msg>
							</g:CheckBox>
						</g:Cell>
						<g:Cell width="100">
							<g:CheckBox ui:field="checkEMailed">
								<ui:msg>E-mailed</ui:msg>
							</g:CheckBox>
						</g:Cell>
						<g:Button ui:field="buttonReportRetrace" width="70px">
							<ui:msg>Retrace</ui:msg>
						</g:Button>
						<g:Button ui:field="buttonReportEmail" width="70px">
							<ui:msg>E-Mail</ui:msg>
						</g:Button>
						<g:Button ui:field="buttonDeleteReport" width="70px">
							<ui:msg>Delete</ui:msg>
						</g:Button>
					</g:HorizontalPanel>
				</g:FlowPanel>
			</g:north>
			<g:center>
				<g:TabLayoutPanel barHeight="3" barUnit="EM"
					ui:field="tabPanel" width="100%" height="99%">
					<g:tab>
						<g:header>
							<ui:msg>Stack Trace</ui:msg>
						</g:header>
						<g:ScrollPanel>
							<g:VerticalPanel width="100%">
								<g:DisclosurePanel open="true" width="100%" ui:field="disStackTrace">
									<g:header>
										<ui:msg>Remapped stack trace</ui:msg>
									</g:header>
									<g:TextArea ui:field="textStackTrace" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%" ui:field="disRawStackTrace">
									<g:header>
										<ui:msg>Original Stack Strace</ui:msg>
									</g:header>
									<g:TextArea ui:field="textRawStackTrace" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
							</g:VerticalPanel>
						</g:ScrollPanel>
					</g:tab>
					<g:tab>
						<g:header>
							<ui:msg>Device</ui:msg>
						</g:header>
						<g:ScrollPanel>
							<g:VerticalPanel width="100%">
								<g:DisclosurePanel open="true" width="100%">
									<g:header>
										<ui:msg>Display</ui:msg>
									</g:header>
									<g:SimplePanel ui:field="tabDisplay" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Initial Configuration</ui:msg>
									</g:header>
									<g:SimplePanel ui:field="tabInitialConfiguration" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Crash Configuration</ui:msg>
									</g:header>
									<g:SimplePanel ui:field="tabCrashConfiguration" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Build</ui:msg>
									</g:header>
									<g:SimplePanel ui:field="tabBuild" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Device Features</ui:msg>
									</g:header>
									<g:Tree ui:field="tabDeviceFeatures" styleName="monospacetext" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Environment</ui:msg>
									</g:header>
									<g:SimplePanel ui:field="tabEnvironment" />
								</g:DisclosurePanel>
							</g:VerticalPanel>
						</g:ScrollPanel>

					</g:tab>
					<g:tab>
						<g:header>
							<ui:msg>Custom Data</ui:msg>
						</g:header>
						<g:TextArea ui:field="textCustomData" styleName="monospacetext" />
					</g:tab>
					<g:tab>
						<g:header>
							<ui:msg>User Comment</ui:msg>
						</g:header>
						<g:TextArea ui:field="tabUserComment" styleName="monospacetext" />
					</g:tab>
					<g:tab>
						<g:header>
							<ui:msg>Logs</ui:msg>
						</g:header>
						<g:ScrollPanel>
							<g:VerticalPanel width="100%">
								<g:DisclosurePanel open="true" width="100%">
									<g:header>
										<ui:msg>Application</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabLogApplication" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Logcat</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabLogcat" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Events</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabLogEvents" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Radio</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabLogRadio" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Dump Sys Mem Info</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabDumpSysMemInfo" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" width="100%">
									<g:header>
										<ui:msg>Dropbox</ui:msg>
									</g:header>
									<g:TextArea ui:field="tabLogDropbox" styleName="monospacetext"
										width="98%" visibleLines="40" />
								</g:DisclosurePanel>
							</g:VerticalPanel>
						</g:ScrollPanel>
					</g:tab>
					<g:tab>
						<g:header>
							<ui:msg>Settings</ui:msg>
						</g:header>
						<g:ScrollPanel>
							<g:VerticalPanel>
								<g:DisclosurePanel open="true" ui:field="tabSharedPreferences">
									<g:header>
										<ui:msg>Shared Preferences</ui:msg>
									</g:header>
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" ui:field="tabSettingsSystem">
									<g:header>
										<ui:msg>System Settings</ui:msg>
									</g:header>
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" ui:field="tabSettingsSecure">
									<g:header>
										<ui:msg>Secure Settings</ui:msg>
									</g:header>
								</g:DisclosurePanel>
								<g:DisclosurePanel open="false" ui:field="tabSettingsGlobal">
									<g:header>
										<ui:msg>Global Settings</ui:msg>
									</g:header>
								</g:DisclosurePanel>
							</g:VerticalPanel>
						</g:ScrollPanel>
					</g:tab>

				</g:TabLayoutPanel>
			</g:center>
		</g:DockLayoutPanel>
	</g:VerticalPanel>
</ui:UiBinder> 