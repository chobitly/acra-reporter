<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:VerticalPanel>
		<g:CaptionPanel captionText="User">
			<g:Grid cellSpacing="4">
				<g:row>
					<g:customCell>
						<g:Label text="E-Mail Address:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textEMailAddress" visibleLength="50" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="First Name:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textFirstName" visibleLength="50" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Last Name:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textLastName" visibleLength="50" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="City:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textCity" visibleLength="50" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Country:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textCountry" visibleLength="50" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:CaptionPanel>
		<g:CaptionPanel captionText="Proguard Mapping Upload Script Authentication">
			<g:Grid cellSpacing="4">
				<g:row>
					<g:customCell>
						<g:Label text="Username:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textAuthUsername" visibleLength="50" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Password:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:PasswordTextBox ui:field="textAuthPassword"
							visibleLength="50" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:CaptionPanel>
		<g:CaptionPanel captionText="Goggle Analytics">
			<g:Grid cellSpacing="4">
				<g:row>
					<g:customCell>
						<g:Label text="Tracking ID:" horizontalAlignment="ALIGN_RIGHT" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="textTrackingID" visibleLength="50" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:CaptionPanel>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:HorizontalPanel spacing="4" horizontalAlignment="ALIGN_CENTER">
				<g:Button ui:field="buttonOK" width="80px">OK</g:Button>
				<g:Label width="20px" />
				<g:Button width="80px" ui:field="buttonCancel">Cancel</g:Button>
			</g:HorizontalPanel>
		</g:Cell>

	</g:VerticalPanel>

</ui:UiBinder> 