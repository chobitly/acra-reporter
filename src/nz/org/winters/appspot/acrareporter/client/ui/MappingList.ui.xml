<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>

	</ui:style>
	<g:DockLayoutPanel unit="EM" width="100%" height="100%">
		<g:north size="2.5">
			<g:MenuBar>
				<g:MenuItem text="Upload" ui:field="itemUpload" />
				<g:MenuItem text="Edit" ui:field="itemEdit" />
				<g:MenuItem text="Delete" ui:field="itemDelete" />
			</g:MenuBar>
		</g:north>
		<g:south size="2.5">
			<g:HorizontalPanel spacing="4" horizontalAlignment="ALIGN_CENTER">
				<g:Button ui:field="buttonClose" width="80">Close</g:Button>
			</g:HorizontalPanel>
		</g:south>
		<g:center>
			<p1:DataGrid ui:field="dataGrid" width="100%"
				height="100%" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 