<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel spacing="10" width="100%" height="100%">
		<g:Label text="ACRA Reporter" styleName="gwt-LabelBig"/>
		<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel spacing="10">
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
				<g:Button text="Login" ui:field="buttonLogin"/>
			</g:Cell>
		</g:HorizontalPanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
				<g:Button text="Wiki" ui:field="buttonWiki"/>
			</g:Cell>
		</g:HorizontalPanel>
				</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
					<g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
				<g:Button text="Sign up" ui:field="buttonSignUp"/>
			</g:Cell>
		</g:HorizontalPanel>
				</g:Cell>
			</g:VerticalPanel>
		</g:Cell>
		<g:Cell verticalAlignment="ALIGN_BOTTOM" horizontalAlignment="ALIGN_RIGHT">
			<g:HorizontalPanel spacing="2">
				<g:Hyperlink text="winters.org.nz" targetHistoryToken="newHistoryToken"/>
			</g:HorizontalPanel>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 