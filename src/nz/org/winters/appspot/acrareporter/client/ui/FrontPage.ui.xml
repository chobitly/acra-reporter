<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel spacing="10" width="100%" height="100%">
		<g:Label styleName="gwt-LabelBig">
			<ui:msg>ACRA Reporter</ui:msg>
		</g:Label>
		<g:Cell width="100%" horizontalAlignment="ALIGN_CENTER"
			verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel spacing="10">
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
							<g:Button ui:field="buttonLogin" width="100px">
								<ui:msg>Login</ui:msg>
							</g:Button>
						</g:Cell>
					</g:HorizontalPanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
							<g:Button ui:field="buttonWiki" width="100px">
								<ui:msg>Wiki</ui:msg>
							</g:Button>
						</g:Cell>
					</g:HorizontalPanel>
				</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
					<g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_CENTER" width="100px">
							<g:Button ui:field="buttonSignUp" width="100px">
								<ui:msg>Sign Up</ui:msg>
							</g:Button>
						</g:Cell>
					</g:HorizontalPanel>
				</g:Cell>
			</g:VerticalPanel>
		</g:Cell>
		<g:Cell verticalAlignment="ALIGN_BOTTOM" horizontalAlignment="ALIGN_RIGHT">
			<g:HorizontalPanel spacing="2">
				<g:Hyperlink text="winters.org.nz" targetHistoryToken="newHistoryToken" />
			</g:HorizontalPanel>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 