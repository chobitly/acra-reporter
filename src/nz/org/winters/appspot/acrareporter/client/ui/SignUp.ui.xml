<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_CENTER"
		verticalAlignment="ALIGN_MIDDLE">
		<g:Cell width="600">
			<g:VerticalPanel width="577px" spacing="4" verticalAlignment="ALIGN_MIDDLE">
			<g:Label styleName="gwt-LabelBig" text="ACRA Reporter - Sign Up" />
			<g:CaptionPanel captionText="User">
				<g:Grid cellSpacing="4">
					<g:row>
						<g:customCell>
							<g:Label text="E-Mail Address:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textEMailAddress" visibleLength="50" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="First Name:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textFirstName" visibleLength="50" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="Last Name:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textLastName" visibleLength="50" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="City:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textCity" visibleLength="50" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="Country:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textCountry" visibleLength="50" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:CaptionPanel>
			<g:CaptionPanel captionText="Proguard Mapping Upload Script Authentication">
				<g:Grid cellSpacing="4">
					<g:row>
						<g:customCell>
							<g:Label text="Username:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textAuthUsername" visibleLength="50" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="Password:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:PasswordTextBox ui:field="textAuthPassword"
								visibleLength="50" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:CaptionPanel>
			<g:CaptionPanel captionText="Goggle Analytics">
				<g:Grid cellSpacing="4">
					<g:row>
						<g:customCell>
							<g:Label text="Tracking ID:" horizontalAlignment="ALIGN_RIGHT" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="textTrackingID" visibleLength="50" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:CaptionPanel>
			<g:InlineHTML ui:field="htmlStuff">arrgghh...</g:InlineHTML>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel spacing="4"
					horizontalAlignment="ALIGN_CENTER">
					<g:Cell width="">
						<g:Button ui:field="buttonOK" width="80">OK</g:Button>
					</g:Cell>
					<g:Button width="80" ui:field="buttonCancel">Cancel</g:Button>
				</g:HorizontalPanel>
			</g:Cell>

		</g:VerticalPanel>
		</g:Cell>

	</g:HorizontalPanel>
</ui:UiBinder> 